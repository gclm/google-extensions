{
  "name": "Github 加速器",
  "description": "Github 加速器, Github 加速",
  "update_url": "https://github.com/gclm/github-booster",
  "version": "1.1.1",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "github-booster.js"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "icons": {
    "128": "github-booster.png"
  },
  "browser_action": {
    "default_icon": "github-booster.png",
    "default_title": "Github 加速器"
  }
}