{
    "background": {
        "scripts": ["js/sea.js", "js/bg.js"]
    },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "description": "最简单易用的google访问助手,为chrome扩展用户量身打造。可以解决chrome扩展无法自动更新的问题，同时可以访问谷歌google搜索，gmail邮箱，google+等谷歌服务。",
    "icons": {
        "128": "images/icon.png"
    },
    "options_page": "options.html",
    "content_scripts": [{
        "all_frames": false,
        "js": ["js/straightGoogle.js"],
        "matches": ["http://*/*", "https://*/*"],
        "run_at": "document_end"
    }],
    "manifest_version": 2,
    "minimum_chrome_version": "18.0.0",
    "name": "google访问助手",
    "permissions": ["proxy", "tabs", "contextMenus", "webRequest", "webRequestBlocking", "webNavigation", "unlimitedStorage", "notifications", "\u003Call_urls\u003E", "http://*/*", "https://*/*", "ftp://*/*"],
    "web_accessible_resources": ["first.html"],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
    "version": "2.3.2"
}